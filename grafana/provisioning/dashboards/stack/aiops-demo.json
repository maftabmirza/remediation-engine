{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": "-- Grafana --",
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "target": {
          "limit": 100,
          "matchAny": false,
          "tags": [],
          "type": "dashboard"
        },
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "id": null,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "type": "stat",
      "title": "AIOps Service Up (Prometheus)",
      "datasource": "Prometheus",
      "gridPos": { "h": 5, "w": 6, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "up{job=\"aiops\"}",
          "legendFormat": "{{instance}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "green", "value": 1 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto",
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "orientation": "auto",
        "textMode": "auto"
      }
    },
    {
      "type": "timeseries",
      "title": "HTTP Requests / sec (all)",
      "datasource": "Prometheus",
      "gridPos": { "h": 9, "w": 12, "x": 6, "y": 0 },
      "targets": [
        {
          "expr": "sum(rate(aiops_http_requests_total[5m]))",
          "legendFormat": "req/s",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "reqps" }, "overrides": [] },
      "options": { "legend": { "displayMode": "list", "placement": "bottom" } }
    },
    {
      "type": "stat",
      "title": "Active Terminal Sessions",
      "datasource": "Prometheus",
      "gridPos": { "h": 5, "w": 6, "x": 18, "y": 0 },
      "targets": [
        {
          "expr": "aiops_terminal_sessions_active",
          "legendFormat": "active",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "none",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "orange", "value": 1 },
              { "color": "red", "value": 10 }
            ]
          }
        },
        "overrides": []
      },
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }
      }
    },
    {
      "type": "timeseries",
      "title": "Alerts Received / sec by Severity",
      "datasource": "Prometheus",
      "gridPos": { "h": 9, "w": 12, "x": 0, "y": 5 },
      "targets": [
        {
          "expr": "sum(rate(aiops_alerts_received_total[5m])) by (severity)",
          "legendFormat": "{{severity}}",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "ops" }, "overrides": [] },
      "options": { "legend": { "displayMode": "list", "placement": "bottom" } }
    },
    {
      "type": "timeseries",
      "title": "Webhook Processing p95 (seconds)",
      "datasource": "Prometheus",
      "gridPos": { "h": 9, "w": 12, "x": 12, "y": 5 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(aiops_webhook_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p95",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "s" }, "overrides": [] },
      "options": { "legend": { "displayMode": "list", "placement": "bottom" } }
    },
    {
      "type": "timeseries",
      "title": "Log Volume (remediation-engine)",
      "datasource": "Loki",
      "gridPos": { "h": 9, "w": 12, "x": 0, "y": 14 },
      "targets": [
        {
          "expr": "sum(count_over_time({container=\"remediation-engine\"}[5m]))",
          "legendFormat": "logs/5m",
          "refId": "A"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "short" }, "overrides": [] },
      "options": { "legend": { "displayMode": "list", "placement": "bottom" } }
    },
    {
      "type": "logs",
      "title": "Recent Errors (remediation-engine)",
      "datasource": "Loki",
      "gridPos": { "h": 9, "w": 12, "x": 12, "y": 14 },
      "targets": [
        {
          "expr": "{container=\"remediation-engine\"} |= \"ERROR\"",
          "refId": "A"
        }
      ],
      "options": {
        "showTime": true,
        "wrapLogMessage": true,
        "showLabels": false,
        "dedupStrategy": "none",
        "enableLogDetails": true
      }
    }
  ],
  "refresh": "10s",
  "schemaVersion": 39,
  "style": "dark",
  "tags": ["demo", "aiops", "prometheus", "loki"],
  "templating": { "list": [] },
  "time": { "from": "now-30m", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "title": "AIOps Demo (Prometheus + Loki)",
  "uid": "aiops-demo",
  "version": 1
}
